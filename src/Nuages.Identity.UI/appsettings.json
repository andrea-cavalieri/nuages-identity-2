{
  "XRay": {
    "DisableXRayTracing": "true",
    "UseRuntimeErrors": "true",
    "CollectSqlQueries": "false"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "webOptimizer": {
    "enableCaching": true,
    "enableMemoryCache": true,
    "enableDiskCache": false,
    "cacheDirectory": "/var/temp/weboptimizercache",
    "enableTagHelperBundling": true,
    "xxxcdnUrl": "https://my-cdn.com/",
    "allowEmptyBundle": false
  },
  "Nuages": {
    "MessageService":
    {
      "SendFromEmail" : "",
      "DefaultCulture" : "en"
    },
    "Localization": {
      "DefaultCulture": "fr-CA",
      "LangClaim": "lang",
      "MissingTranslationUrl": null,
      "Cultures": [
        "fr-CA",
        "en-CA"
      ]
    },
    "UI":
    {
      "ShowRegistration" : true,
      "ExternalLoginAutoEnrollIfEmailExists" : true,
      "ExternalLoginPersistent" : true,
      "EnablePasswordless" : true,
      "EnablePhoneFallback" : true,
      "Enable2FARememberDevice" : true,
      "EnableFido2" : true,
      "FontAwesomeUrl" : "https://kit.fontawesome.com/70b74b4315.js"
    }
  },
  "ApplicationConfig": {
    "ParameterStore": {
      "Enabled": true,
      "Path": "/NuagesIdentity"
    },
    "AppConfig": {
      "Enabled": true,
      "ApplicationId": "NuagesIdentity",
      "EnvironmentId": "Prod",
      "ConfigProfileId": "WebUI"
    }
  },
  "Identity": {
    "Name": "Nuages",
    "Authority": "https://localhost:8001",
    "SupportsAutoPasswordExpiration": true,
    "AutoExpirePasswordDelayInDays": 60,
    "SupportsLoginWithEmail": true,
    "AutoConfirmExternalLogin": true,
    "Audiences": ["MyAudience"],
    "Password": {
      "RequiredLength": 6,
      "RequireNonAlphanumeric": true,
      "RequireLowercase": true,
      "RequireUppercase": true,
      "RequireDigit": true,
      "RequiredUniqueChars": 1
    },
    "Web":
    {
      "GoogleRecaptcha" :
      {
        "SiteKey" : null,
        "SecretKey" : null
      }
    }
  },
  "Google" :
  {
    "ClientId" : null,
    "ClientSecret" : null
  },
  "fido2": {
    "serverDomain": "localhost",
    "origin": "https://localhost:8001",
    "timestampDriftTolerance": 300000,
    "serverName" : "Nuages"
  }
  
}
