@inject IOptions<Nuages.Web.Recaptcha.GoogleRecaptchaOptions> RecaptchaOptions

<!DOCTYPE html>
<html lang="en" class="dark ">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>@ViewData["Title"] - Nuages.Identity.UI</title>

    <link rel="stylesheet" href="~/css/site.css"/>
    <link rel="stylesheet" href="~/css/tailwind-output.css"/>
    <script src="https://kit.fontawesome.com/34d2a269b0.js" crossorigin="anonymous"></script>
    
</head>
<body class="w-full">

<partial name="_NavBar"/>
<div class="min-h-screen flex flex-col justify-start max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
    <div class="mt-4">
         
       
       <div class="flex flex-row">
           <div class="basis-1/4">
               
               <ul class="list-disc">
                  
                   <li> <a href="~/account/manage/changePassword">@Localizer["changePassword:title"]</a></li>
                   <li> <a href="~/account/manage/email">@Localizer["changeEmail:title"]</a></li>
                   <li> <a href="~/account/manage/username">@Localizer["changeUsername:title"]</a></li>
                   <li> <a href="~/account/manage/twoFactorAuthentication">@Localizer["mfa:title"]</a></li>
                   <li> <a href="~/account/manage/externalLogins">@Localizer["externalLogin:shortTitle"]</a></li>
                   <li> <a href="~/account/manage/profile">@Localizer["profile:title"]</a></li>
               </ul>
               @RenderSection("LeftNav", false)
           </div>
           <div class="basis-1/2"> @RenderBody()</div>
          
           <div class="basis-1/4">
               @RenderSection("RightNav", false)
           </div>
       </div>
        
    </div>
</div>

<environment names="Production">
    <script src="https://unpkg.com/vue@3/dist/vue.global.prod.js"></script>
</environment>
<environment names="Development">
    <script src="https://unpkg.com/vue@3"></script>
</environment>

@RenderSection("Scripts", false)
</body>
</html>