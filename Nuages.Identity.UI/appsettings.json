{
  "XRay": {
    "DisableXRayTracing": "true",
    "UseRuntimeErrors": "true",
    "CollectSqlQueries": "false"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "webOptimizer": {
    "enableCaching": true,
    "enableMemoryCache": true,
    "enableDiskCache": false,
    "cacheDirectory": "/var/temp/weboptimizercache",
    "enableTagHelperBundling": true,
    "xxxcdnUrl": "https://my-cdn.com/",
    "allowEmptyBundle": false
  },
  "Nuages": {
    "Localization": {
      "DefaultCulture": "fr-CA",
      "LangClaim": "lang",
      "MissingTranslationUrl": null,
      "Cultures": [
        "fr-CA",
        "en-CA"
      ]
    }
  },
  "ApplicationConfig": {
    "ParameterStore": {
      "Enabled": true,
      "Path": "/NuagesIdentity"
    },
    "AppConfig": {
      "Enabled": true,
      "ApplicationId": "NuagesIdentity",
      "EnvironmentId": "Prod",
      "ConfigProfileId": "WebUI"
    }
  }
}
